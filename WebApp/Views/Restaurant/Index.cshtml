@model IEnumerable<DTO.City>

@{
    ViewData["Title"] = "Index";
}

<h1>list of citys</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CITYNAME)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NPA)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CITYNAME)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NPA)
            </td>
            <td>
                @Html.ActionLink("view more", "RestaurantsList", new { idCity = item.IDCITY})

            </td>
        </tr>
}
    </tbody>
</table>
